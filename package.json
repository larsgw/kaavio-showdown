{
  "private": true,
  "name": "@wikipathways/kaavio-showdown",
  "version": "0.0.5",
  "description": "Extended MarkDown syntax for Kaavio and the Manipulation API",
  "repository": "jacobwindsor/kaavio-showdown",
  "author": "Jacob Windsor",
  "license": "MIT",
  "keywords": [
    "Kaavio",
    "diagram",
    "markdown",
    "showdown"
  ],
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es6.js",
  "module": "index.es6.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "showdown": "^1.6.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "del": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "rollup": "^0.26.2",
    "rollup-plugin-babel": "^2.4.0",
    "should": "^11.2.1",
    "sinon": "^2.0.0-pre"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "prepublish": "npm run build",
    "publish:docs": "easystatic deploy docs --repo jacobwindsor/kaavio-markdown",
    "start": "easystatic start docs"
  }
}
